{
  "name": "realcredmais",
  "compatibility_date": "2025-07-17",
  "pages_build_output_dir": "dist",
  "vars": {
    "NODE_VERSION": "22.16.0"
  },
  "build": {
    "command": "npm run build",
    "cwd": ".",
    "watch_directories": ["src"],
    "upload": {
      "format": "service-worker"
    }
  },
  "env": {
    "production": {
      "route": "realcredmais.pages.dev/*",
      "vars": {
        "ENVIRONMENT": "production"
      }
    }
  }
}
